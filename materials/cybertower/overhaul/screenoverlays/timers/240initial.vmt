UnlitGeneric
{
	$basetexture "cybertower/overhaul/screenoverlays/timers/numbahz"

	$detail "cybertower/screenoverlays/countdown/flashmask"
	$detailblendmode 8
	"$color2" "[ 255 0 0 ]"

	$alphatest 1

	$basetexturetransform "center 0.5 0.5 scale 7 8 rotate 0 translate 0 2.3"

	$nolod 1

	$frame 240

	Proxies
	{
		AnimatedTexture
		{
			animatedTextureVar "$detail"
			animatedTextureFrameNumVar "$detailframe"
			animatedTextureFrameRate 8
		}
	}
}