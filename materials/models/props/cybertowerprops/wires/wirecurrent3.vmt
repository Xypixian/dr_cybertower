UnlitGeneric
{
	$basetexture "cybertower/towerskybox/wirecurrent"

	$nolod 1

	$color2 "[1 0.77 0.08]"

	$rotvar 90
	$scavar 0.1
	$scrollvar "[ 0 0 ]"

	Proxies
	{
		TextureScroll
		{
			texturescrollvar "$scrollvar"
			texturescrollrate 1
			texturescrollangle -90
		}

		TextureTransform
		{
			translateVar "$scrollVar"
			rotateVar "$rotVar"
			scaleVar "$scaVar"
			resultVar "$basetexturetransform"
		}
	}
}